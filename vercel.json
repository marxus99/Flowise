{
    "buildCommand": "npm run build:components && npm run build:ui",
    "outputDirectory": "packages/ui/build",
    "installCommand": "npm install",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://flowise-ai-cqlx.onrender.com/api/v1/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}
